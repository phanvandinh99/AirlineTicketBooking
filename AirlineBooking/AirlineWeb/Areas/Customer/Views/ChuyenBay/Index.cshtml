    <script>
        $(document).ready(function () {
            function loadDanhSachSanBay() {
                $.ajax({
                    url: '/Customer/Home/GetDanhSachSanBay',
                    method: 'GET',
                    success: function (res) {
                        try {
                            if (res.success && res.data) {
                                var options = '<option value="">Chọn sân bay</option>';
                                res.data.forEach(function (airport) {
                                    var city = airport.ThanhPho ? airport.ThanhPho + ' - ' : '';
                                    options += '<option value="' + airport.MaSanBay + '">' + city + airport.TenSanBay + ' (' + airport.MaSanBay + ')</option>';
                                });
                                $('#from-airport').html(options);
                                $('#to-airport').html('<option value="">Chọn sân bay đến</option>');
                            } else {
                                console.warn('Lấy danh sách sân bay thất bại:', res.message);
                            }
                        } catch (e) {
                            console.error('Lỗi xử lý dữ liệu sân bay:', e, res);
                        }
                    },
                    error: function (xhr, status, error) {
                        console.error('API call failed:', status, error, xhr);
                        alert('Không thể lấy dữ liệu sân bay từ server!');
                    }
                });
            }
            loadDanhSachSanBay();
            $('#from-airport').on('change', function () {
                var fromValue = $(this).val();
                if (fromValue) {
                    $.ajax({
                        url: '/Customer/Home/GetSanBayDen',
                        method: 'GET',
                        data: { maSanBayDi: fromValue },
                        success: function (res) {
                            if (res.success && res.data) {
                                var options = '<option value="">Chọn sân bay đến</option>';
                                res.data.forEach(function (airport) {
                                    var city = airport.ThanhPho ? airport.ThanhPho + ' - ' : '';
                                    options += '<option value="' + airport.MaSanBay + '">' + city + airport.TenSanBay + ' (' + airport.MaSanBay + ')</option>';
                                });
                                $('#to-airport').html(options);
                                $('#to-airport').prop('disabled', false);
                            } else {
                                $('#to-airport').html('<option value="">Không có sân bay đến</option>');
                            }
                        },
                        error: function () {
                            $('#to-airport').html('<option value="">Lỗi tải dữ liệu</option>');
                        }
                    });
                } else {
                    $('#to-airport').html('<option value="">Chọn sân bay đến</option>');
                    $('#to-airport').prop('disabled', true);
                }
            });
        });
    </script>
